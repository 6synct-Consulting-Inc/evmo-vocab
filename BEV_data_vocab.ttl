  skos:prefLabel "Mean Operating Time"@en;
  skos:definition "The average time spent operating (Average of Operating Time)"@en;
  skos:notation "MOT"@en;
  skos:inScheme <https://zero.nexus/wp-content/uploads/2025/10/bev.ttl> .

