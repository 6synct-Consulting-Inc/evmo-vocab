  skos:member bev:battery_id, bev:model_year, bev:initial_service_year, bev:model_name,
    bev:manufacturer, bev:functional_location, bev:purchase_cost, bev:measurement_start,
    bev:measurement_end, bev:alert_id, bev:alert_type, bev:alert_description, bev:alert_date_time,
    bev:alert_status, bev:pack_id, bev:module_id, bev:cell_id, bev:battery_chemistry,
