    bev:curb_weight, bev:gross_vehicle_weight, bev:purchase_cost, bev:nominal_range, bev:nominal_efficiency,
    bev:peak_power, bev:peak_torque, bev:auxiliary_powered_heater, bev:max_charge_rate,
    <https://zero.nexus/bev-data-vocabulary/charge connector>, bev:measurement_start,
    bev:measurement_end, bev:vehicle_hours, bev:total_distance, bev:vehicle_idling_time,
    bev:driving_time, bev:total_run_time, bev:queue_time, bev:battery_swapping_time, bev:start_location,
    bev:end_location, bev:location, bev:average_speed, bev:speed, bev:acceleration, bev:number_of_charging_sessions,
